{
  "kind": "collectionType",
  "collectionName": "patrons",
  "info": {
    "singularName": "patron",
    "pluralName": "patrons",
    "displayName": "Patron"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "rollNo": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Phone": {
      "type": "string",
      "required": true
    },
    "Role": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Student",
        "Faculty",
        "Staff",
        "Guest"
      ]
    },
    "Department": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "CSE",
        "ECE",
        "MBA"
      ]
    },
    "Course": {
      "type": "string",
      "required": true
    },
    "year": {
      "type": "integer"
    },
    "Gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Other"
      ]
    },
    "dateOfRegistration": {
      "type": "date",
      "required": true
    },
    "Address": {
      "type": "text"
    },
    "patronStatus": {
      "type": "enumeration",
      "required": true,
      "default": "Active",
      "enum": [
        "Active",
        "Blocked"
      ]
    },
    "profile": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "branch": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::branch.branch"
    }
  }
}
